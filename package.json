    "@signalk/github-create-release": "^1.2.0",
    "github-changes": "^2.0.3",
    "mocha": "^10.2.0",
    "prettier-standard": "^15.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenFairWind/signalk-to-stalk.git"
  },
  "bugs": {
